{
  "name": "reactjs-wiki",
  "version": "0.1.0",
  "description": "Single-page app utilizing Webpack, React.js, Redux, ES6, WikiMedia API",
  "scripts": {
    "start": "node server.js",
    "test:unix": "NODE_ENV=test mocha --recursive --compilers js:babel-core/register",
    "test:win": "set NODE_ENV=test && mocha --recursive --compilers js:babel-core/register",
    "test:unix:watch": "npm run test:unix -- --watch",
    "test:win:watch": "npm run test:win -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dimadesu/reactjs-wiki.git"
  },
  "author": "Dmitry Antonov <antonov.dmitriy@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dimadesu/reactjs-wiki/issues"
  },
  "homepage": "https://github.com/dimadesu/reactjs-wiki",
  "dependencies": {
    "classnames": "2.1.2",
    "react": "0.13.3",
    "react-bootstrap": "0.26.4",
    "react-redux": "2.1.2",
    "redux": "3.0.0",
    "redux-thunk": "1.0.0",
    "superagent": "1.4.0",
    "superagent-jsonp": "0.0.6"
  },
  "devDependencies": {
    "babel-core": "5.6.18",
    "babel-loader": "5.1.4",
    "babel-plugin-react-transform": "1.1.0",
    "expect": "1.8.0",
    "express": "4.13.3",
    "jsdom": "5.6.1",
    "mocha": "2.2.5",
    "mocha-jsdom": "1.0.0",
    "node-libs-browser": "0.5.2",
    "raw-loader": "0.5.1",
    "react-transform-hmr": "1.0.0",
    "style-loader": "0.12.3",
    "webpack": "1.9.11",
    "webpack-dev-middleware": "1.2.0",
    "webpack-hot-middleware": "2.2.0"
  }
}
